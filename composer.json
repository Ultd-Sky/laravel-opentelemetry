{
    "name": "keepsuit/laravel-opentelemetry",
    "description": "OpenTelemetry integration for laravel",
    "keywords": [
        "keepsuit",
        "laravel",
        "laravel-opentelemetry"
    ],
    "homepage": "https://github.com/keepsuit/laravel-opentelemetry",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Capucci",
            "email": "f.capucci@keepsuit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Keepsuit\\LaravelOpentelemetry\\": "src",
            "Keepsuit\\LaravelOpentelemetry\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keepsuit\\LaravelOpentelemetry\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Keepsuit\\LaravelOpentelemetry\\LaravelOpentelemetryServiceProvider"
            ],
            "aliases": {
                "LaravelOpentelemetry": "Keepsuit\\LaravelOpentelemetry\\LaravelOpentelemetryFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
